{
  "name": "vue3-bulma-template",
  "version": "0.0.0",
  "scripts": {
    "serve": "npx vite --port 8080",
    "lint": "npx eslint . --ext .vue,.js,.ts,.cjs,.mjs --fix --ignore-path .gitignore",
    "preview": "npx vite preview --port 8080",
    "build": "npx vite build",
    "build:gh-pages": "npx vite build --outDir ./docs",
    "deploy:gh-pages": "npx simple-github-pages-deploy"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "pinia": "^2.0.14",
    "pinia-plugin-persistedstate": "^1.6.1",
    "simpler-fetch": "^6.1.1",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.17.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.11"
  }
}
